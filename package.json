    "deploy": "npm run save && railway up",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "sora:generate": "npx tsx scripts/sora-web-generator.ts"
